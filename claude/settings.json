{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1",
    "PLANNOTATOR_PORT": "19432"
  },
  "model": "opusplan",
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "sh -c '$HOME/dotfiles/claude/notification-wrapper.sh'"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "sh -c '$HOME/dotfiles/claude/statusline-wrapper.sh'",
    "padding": 0
  },
  "enabledPlugins": {},
  "language": "Korean",
  "alwaysThinkingEnabled": true,
  "autoUpdatesChannel": "latest",
  "skipDangerousModePermissionPrompt": true,
  "autoCompact": false,
  "feedbackSurveyState": {
    "lastShownTime": 1754195027959
  },
  "effortLevel": "low"
}
