{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "sh -c '$HOME/dotfiles/claude/notification-wrapper.sh'"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "sh -c '$HOME/dotfiles/claude/hooks/pre-push-gate.sh'"
          },
          {
            "type": "command",
            "command": "sh -c '$HOME/dotfiles/claude/hooks/require-issue-link.sh'"
          },
          {
            "type": "command",
            "command": "sh -c '$HOME/dotfiles/claude/hooks/ddb-readonly-gate.sh'"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "sh -c '$HOME/dotfiles/claude/statusline-wrapper.sh'",
    "padding": 0
  },
  "enabledPlugins": {
    "commit-commands@claude-plugins-official": true,
    "Notion@claude-plugins-official": true,
    "security-guidance@claude-plugins-official": true,
    "hookify@claude-plugins-official": true,
    "sentry@claude-plugins-official": false,
    "serena@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true
  },
  "language": "Korean",
  "alwaysThinkingEnabled": true,
  "autoUpdatesChannel": "latest",
  "skipDangerousModePermissionPrompt": true,
  "autoCompact": false,
  "feedbackSurveyState": {
    "lastShownTime": 1754195027959
  },
  "effortLevel": "medium"
}
